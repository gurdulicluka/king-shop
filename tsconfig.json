{
	"compilerOptions": {
		"jsx": "react-jsx",
		"module": "ESNext",
		"lib": ["dom", "es2019", "DOM.Iterable"],
		"skipLibCheck": true,
		"moduleResolution": "node",
		"types": ["vite/client"],
		"allowImportingTsExtensions": true,
		"noEmit": true
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules", "dist"]
}
